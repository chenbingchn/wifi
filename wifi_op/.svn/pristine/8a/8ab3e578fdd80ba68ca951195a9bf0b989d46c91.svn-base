CREATE DATABASE instagram;

USE instagram;

CREATE TABLE user(
        user_id INT NOT NULL AUTO_INCREMENT,
        user_name VARCHAR(50) NOT NULL,
        PRIMARY KEY(user_id)
) DEFAULT CHARSET=utf8;

CREATE TABLE image(
        id INT NOT NULL AUTO_INCREMENT,
        url VARCHAR(255) NOT NULL UNIQUE,
        PRIMARY KEY(id)
) DEFAULT CHARSET=utf8;

