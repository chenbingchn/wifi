.cover {
  width: 100%;
  height: 100%;
  overflow: hidden;
  background: #000000;
  opacity: 0.6;
  position: fixed;
  top: 0px;
  left: 0px;
  display: none;
}
.alert {
  display: none;
  position: fixed;
  background-color: white;
  top: 30%;
  left: 10%;
  width: 80%;
  z-index: 10000;
  border-radius: 15px;
}
.alert .title {
  color: #1c8a00;
  font-family: arial;
  font-size: 16px;
  text-align: center;
  height: 50px;
  line-height: 50px;
  border-bottom: 1px solid #1c8a00;
  position: relative;
}
.alert .title .close {
  width: 25px;
  height: 25px;
  background: url(../images/ico.png) no-repeat right -48px;
  float: left;
  position: absolute;
  top: 8px;
  right: 8px;
}
.alert .content {
  width: 80%;
  min-height: 80px;
  line-height: 30px;
  margin: 0 auto;
  padding-bottom: 20px;
}
.alert .content .goosname {
  color: #fa722c;
}
.alert .content .okalert {
  display: none;
}
.alert .content .checkbtn {
  width: 80%;
  height: 40px;
  line-height: 60px;
  margin: 10px auto;
  border-radius: 5px;
  background: #63b002;
  text-align: center;
  color: #FFFFFF;
  line-height: 40px;
}
.alert .content .tipsbtn {
  width: 80%;
  line-height: 40px;
  height: 40px;
  margin: 0 auto;
  text-align: center;
  color: #FFFFFF;
}
.alert .content .tipsbtn .sure {
  width: 45%;
  height: 40px;
  border-radius: 5px;
  background: #63b002;
  float: left;
}
