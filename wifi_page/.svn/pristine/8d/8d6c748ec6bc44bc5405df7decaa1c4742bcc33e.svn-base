.maincategory{
    width: 25%;
    float: left;
    text-align: center;
    font-size: 12px;
    overflow: visible;
    padding-top: 10px;
    height: 30px;
}
.mainclass{
    height: 30px;
    line-height: 30px;
    font-size: 16px;
    padding-left: 10px;
    padding-right: 10px;
}
.hrstyle{
    margin-top: 0;
    margin-bottom: 0;
    clear: both;
}
.glyphicon{
    position: relative;
    top: -2px;
    display: inline-block;
    font-family: "Glyphicons Halflings";
    font-style: normal;
    font-weight: 400;
    line-height: 1;
    -moz-osx-font-smoothing: grayscale;
}
.container{
    padding: 0;
}