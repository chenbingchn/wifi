@charset "utf-8";
/* CSS Document */


a {
	color: #000;
}
a:focus {
	outline: 0;
}
a:link {
	text-decoration: none;
}
a:visited {
	text-decoration: none;
}
a:hover {
	color:#000;
	text-decoration: none;
}
a:active {
	text-decoration: none;
}

body{
	background-color:#F0F3F5;
	text-align: left;
}
/*原来个人中心的css有一些可能已经不用了！！*/
.top-colos{
	background-color:#329519;
	height:40px;
}
.top-Earn_colos{
	background-color:#329519;
	height:180px;
}
[class*="col-"]{
	padding-top:5px;
	font-size:15px;
}
.word{
	color:#FFF;
	font-size:20px;
	padding-top: 6px;
	width: 88%;
}
.pic{
	float:left;
	padding-top: 5px;
	width: 10%;
	padding-bottom: 5px;
}
.Earn_pic{
	float:left;
}

.row{
	margin:0;
}
.black{
	background-color: rgba(0,0,0,0.2);
	margin-top:-30px;
	width:100%;
	height:30px;
	color:#FFF;
}
.p{
	float:left;
	margin-left:-10px;
	font-size:12px;
}
.word-color{
	color:#FFF;
}
.top-word{
	color:#FFF;
	font-size:15px;
}
.height{
	background-color:#FFF;
	border-bottom: 1px solid #E0E0E1;
}
.tops{
	border-bottom: 1px solid #E0E0E1;
}
.smalls{
	float: right;
	margin-right: -10px;
	margin-top: -1px;
}
.view-modal{
	margin: 30% auto;
	width: 85%;
	font-size: 12px;
}
.demoy {
	float:left;
	border-radius:40px 40px 40px 40px;
	background-color:#F8931D;
	width:55px;
	height:30px;
	text-align:center;
	color:#FFF;
	margin-top:10px;
	padding-top:4px;
	margin-left:5px;
}
.demogr {
	float:left;
	border-radius:40px 40px 40px 40px;
	background-color:#CCCCCC;
	width:55px;
	height:30px;
	text-align:center;
	color:#FFF;
	margin-top:10px;
	padding-top:4px;
	margin-left:5px;
}
.imgr{
	margin-top:18px;
	float:right;
	margin-right:-15px;
}
.imgt{
	margin-top:6px;
	float:right;
	margin-right:-15px;
}
.container{
	padding-left:0;
	padding-right:0;
}
.contents {
	margin:0 auto;
	z-index: 3;
	margin-top: 35px;
}
.navl {
	width:100%;
	margin:0 auto;
	position:fixed;
	fixed:top;
	top:0;
	text-align:center;
	z-index: 2
}
i.contentrs {
	margin:180px auto 0;
	z-index: 3;
	
}
/*这是新的个人中心的css*/
.head{
	background-color: #329519;
	height: 200px;
}
.headtitle{
	padding-top: 5px;
	padding-bottom: 5px;
}
.headtitleleft{
	float: left;
	width: 10%;
}
.headtitleword{
	float: left;
	width: 90%;
	height: 40px;
	color: #ffffff;
	font-size: 20px;
	padding-right: 10%;
}
.circle {
	border-radius:4px 4px 4px 4px;
	padding:3px 10px 3px 10px;
	border:1px solid #FFF;
	height:30px;
	text-align:center;
	width: 180px;
}

#view{
	 font-size: 14px;
	 margin-top: 200px;
}
#view .list{
	 float: left;
	 width: 100%;
	 background-color: #ffffff;
	 border-bottom: 1px solid #E0E0E1;
	 padding: 5px 0 5px 10px;
	 height: 70px;
}
.list:active,
#view .list:active {
	background-color: #e2e1e1;
}
#view .listNull{
	 float: left;
	 width: 100%;
	 height: 15px;
	 border-bottom: 1px solid #E0E0E1;
	 padding: 5px 0 5px 10px;
}
#view .words{
	 float: left;
	 width: 70%;
	 padding-left: 5px;
	 color:#000;
}
#view .show{
	 float:left;
	 width: 30%;
	 vertical-align:middle;
	 padding-top: 10px;
}
#view .les{
	 float: right;
	 padding-right: 25px;
	 background-image:url(../../images/common/right.png);
	 background-repeat : no-repeat;
	 background-position : 5px 6px;
	 background-size: 28%;
	 margin-top: 5px;
}
#view .lets{
	 float: right;
	 padding-right: 25px;
	 background-image:url(../../images/common/right.png);
	 background-repeat : no-repeat;
	 background-position : 5px 6px ;
	 background-size: 28%;
	 margin-top: 5px;
}
#view .rig{
	 float: right;
	 margin-right: 8%;
	 border-radius: 4px;
	 background-color: #F8931D;
	 color: #FFF;
	 text-align: center;
	 width: 50px;
	 height: 25px;
	 padding-top: 3px;
	 line-height: 20px;
	 margin-top: 5px;
}
#view small{
	 font-size: 10px;
	 color: #777777;
	 line-height: 10px;
}
#view .texts{
	 padding-bottom: 3px;
}


.fire{
	 position: absolute;
	 background-color: rgba(0, 0, 0, 5);
	 opacity: 0.5;
	 z-index: 2;
	 text-align: center;
	 color:#FFF; 
}

/*Time-pick*/
.time-pick-main{
    width: 50%;
    float: left;
}
.time-pick-content{
    padding-bottom: 15px;
}
